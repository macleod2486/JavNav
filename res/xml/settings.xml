<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="map">
        <ListPreference
            android:entries="@array/MapType"
            android:entryValues="@array/selection"
            android:defaultValue="1"
            android:title="Map Type"
            android:key="mapSelect" >
            
        </ListPreference>
    </PreferenceCategory>
    
    <PreferenceCategory android:title="Extra">
        <CheckBoxPreference
            android:title="Custom Webtab"
            android:key="extra"
            android:persistent="true"
            android:defaultValue="false" />
        <EditTextPreference 
            android:key="webURL"
            android:dialogTitle="URL of website"
            android:title="Enter URL of website"/>
    </PreferenceCategory>
    
</PreferenceScreen>